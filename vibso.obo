property_value: seeAlso https://github.com/NFDI4Chem/VibrationalSpectroscopyOntology/issues/46

[Term]
id: VIBSO:0000009
name: groove density
def: "A physical quality inhering in a diffraction grating device by virtue of the device's function to diffract light. This quality specifically depends on the number of grooves per area of the grating device and is usually expressed in grooves per millimeter." [] {dce:source="wikipedia:Diffraction_grating", dce:source=":~:text=The%20groove%20density%20(or%20groove,of%20the%20operational%20wavelength%20range"}
is_a: PATO:0001018 ! physical quality
property_value: dce:contributor ORCID:0000-0002-1595-3213
property_value: IAO:0000116 "temp class until NTR in PATO has been accepted and merged (https://github.com/pato-ontology/pato/issues/541)" xsd:string
property_value: IAO:0000118 "groove frequency" xsd:string
property_value: seeAlso https://github.com/NFDI4Chem/VibrationalSpectroscopyOntology/issues/45

[Term]
id: VIBSO:0000010
